	<parameter name="ParamAnioDesde" class="java.lang.Integer"/>
	<parameter name="ParamAnioHasta" class="java.lang.Integer"/>
	<parameter name="ParamFechaCursada" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[SELECT mesaexamen.alumno.aldni,
	mesaexamen.alumno.alapynom,
	avg( notafinal),
	count(*)
FROM mesaexamen.alumnomateria
	INNER JOIN mesaexamen.alumno ON 
	 mesaexamen.alumnomateria.aldni2 = mesaexamen.alumno.aldni 
	INNER JOIN mesaexamen.materia ON 
	 mesaexamen.alumnomateria.codmat2 = mesaexamen.materia.codmat 
WHERE 
	 mesaexamen.alumnomateria.notafinal >= 4 
	 AND mesaexamen.alumnomateria.fechadecursada BETWEEN $P{ParamAnioDesde} AND $P{ParamAnioHasta} 
	 AND mesaexamen.materia.anio BETWEEN '1' AND '2' 
	 AND mesaexamen.alumnomateria.codmat2 NOT IN (SELECT materia.codmat
FROM materia
	inner join mesaexamen.alumnomateria ON 
	 materia.codmat = mesaexamen.alumnomateria.codmat2 
WHERE 
	 materia.anio = '3' 
	 AND mesaexamen.alumnomateria.fechadecursada = $P{ParamFechaCursada} )
GROUP BY mesaexamen.alumno.aldni]]>
	</queryString>
